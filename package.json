{
  "name": "wonderdep",
  "version": "0.1.17",
  "description": "Found the nearest package version of a specify dep version",
  "keywords": [],
  "homepage": "https://github.com/zoeyzhao19/wonderdep#readme",
  "bugs": "https://github.com/zoeyzhao19/wonderdep/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoeyzhao19/wonderdep.git"
  },
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "wonder": "./bin/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --fix ./src/**",
    "prepare": "husky install",
    "release": "vitest run && changelogen --release && git push --follow-tags && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "pacote": "^17.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.43.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@npmcli/config": "^8.0.2",
    "@types/npmcli__config": "^6.0.3",
    "@types/pacote": "^11.1.8",
    "@types/semver": "^7.5.6",
    "changelogen": "^0.5.5",
    "commitlint": "^18.4.3",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "pnpm@8.11.0"
}